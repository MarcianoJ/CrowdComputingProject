<p id="notice"><%= notice %></p>

<h1>Data Points</h1>

<table>
  <thead>
    <tr>
      <th>Input</th>
      <th>Input2</th>
      <th>Maskable words</th>
      <th>Custom options</th>
      <th>Dataset</th>
      <th>Classification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_points.each do |data_point| %>
      <tr>
        <td><%= data_point.input %></td>
        <td><%= data_point.input2 %></td>
        <td><%= data_point.maskable_words %></td>
        <td><%= data_point.custom_options %></td>
        <td><%= data_point.dataset_id %></td>
        <td><%= data_point.classification %></td>
        <td><%= link_to 'Show', data_point %></td>
        <td><%= link_to 'Edit', edit_data_point_path(data_point) %></td>
        <td><%= link_to 'Destroy', data_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Point', new_data_point_path %>
